<html>
    <head>
        <title>결과</title>    
        <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

       
    </head>
    <body>
        <div id="result">
        
       
        
        </div>    
    </body>   
    <script type="text/javascript">
        var JsonData = document.getElementById("myJsonData");
        var initData = JSON.parse('<%-JSON.stringify(model_result)%>');
        console.log(initData["결과"].length);
        var item = initData["결과"];
        $(function(){
        for(i=0; i<item.length; i++){
            
            if (item[i]["tag"]=="0000") $("#result").append("<font size=3 color=blue>"+item[i]["result"]+" "+"</font>");
            else if (item[i]["tag"]=="1000") $("#result").append("<font size=3 color=black>"+item[i]["result"]+" "+"</font>");
            else if (item[i]["tag"]=="1001") $("#result").append("<font size=3 color=red>"+item[i]["result"]+" "+"</font>");
        }
    });
     
     </script>
</html>